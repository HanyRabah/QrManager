generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String?
  title     String?
  district  String?
  region    String?
  scanned   Boolean?  @default(false)
  scanTime  DateTime?
  location  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}